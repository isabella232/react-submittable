{
  "name": "react-submittable",
  "version": "1.0.0",
  "description": "a replacement for preventing the default behavior of forms: allows submission on enter",
  "main": "dist/submittable.js",
  "scripts": {
    "test": "jest && coveralls < coverage/lcov.info",
    "build": "babel index.js > dist/submittable.js"
  },
  "keywords": [
    "submit",
    "form",
    "element",
    "react"
  ],
  "author": "Tom MacWright",
  "license": "ISC",
  "devDependencies": {
    "jest-cli": "^0.4.18",
    "react-tools": "^0.13.3"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "collectCoverage": true
  },
  "dependencies": {
    "babel": "^5.8.21",
    "coveralls": "^2.11.4",
    "react": "*"
  }
}
